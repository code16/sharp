{
    "name": "code16/sharp-demo",
    "type": "project",
    "require": {
        "php": "^8.4",
        "bacon/bacon-qr-code": "~2.0",
        "blade-ui-kit/blade-icons": "^1.6",
        "code16/laravel-content-renderer": "^1.2",
        "enshrined/svg-sanitize": "^0.21.0",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^2.0",
        "intervention/image": "^3.4",
        "intervention/image-laravel": "^1.2",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.9",
        "masterminds/html5": "^2.8",
        "pragmarx/google2fa": "^8.0",
        "spatie/image-optimizer": "^1.6",
        "spatie/laravel-translatable": "^6.0",
        "symfony/html-sanitizer": "^7.3",
        "technikermathe/blade-lucide-icons": "^3.98",
        "tightenco/ziggy": "^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "itsgoingd/clockwork": "^5.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-ray": "^1.29",
        "spatie/laravel-typescript-transformer": "^2.3"
    },
    "autoload": {
        "files": [
            "../src/sharp_helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Code16\\Sharp\\": "../src/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --tag=sharp-assets --force"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
